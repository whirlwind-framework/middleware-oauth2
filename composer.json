    }
  ],
  "require": {
    "php": "~7.4||>=8.0",
    "ext-json": "*",
    "whirlwind-framework/framework": "*",
    "psr/cache": "^1.0",
    "psr/http-client": "^1.0"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.2",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5"
